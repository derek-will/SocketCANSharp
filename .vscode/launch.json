    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (ISO-TP Simulator)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/examples/IsoTpCommSimulator/bin/Debug/net5.0/IsoTpCommSimulator.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/IsoTpCommSimulator",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Launch (CAN Bus Sniffer)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/examples/CanBusSniffer/bin/Debug/net5.0/CanBusSniffer.dll",
            "args": [],
            "cwd": "${workspaceFolder}/examples/CanBusSniffer",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}